{
    "manifest_version": 2,
    "name": "__MSG_extName__",
    "default_locale": "fr",
    "short_name": "PageAPatte",
    "description": "__MSG_extDescription__",
    "version": "1.2.1.1",
    "permissions": [
        "geolocation",
        "bookmarks",
        "storage",
        "<all_urls>",
        "cookies"
    ],
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}